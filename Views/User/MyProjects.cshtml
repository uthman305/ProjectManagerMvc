@model List<ProjectManager.Dto.ProjectListingViewModel> 
    @{
	Layout = null;
}
<h2>My Projects</h2>

@if (Model.Count > 0)
{
    <ul>
        @foreach (var project in Model)
        {
            <li>
               
                    <a href="@Url.Action("Index", "Project", new { projectId = project.ProjectId })">
                    @project.ProjectName
                </a>
            
            </li>
        }
    </ul>
}
else
{
    <p>You are not a member of any projects.</p>
}